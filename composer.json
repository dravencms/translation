{
  "name": "dravencms/translation",
  "type": "dravencms-package",
  "description": "DravenCMS Kdyby Translation",
  "keywords": [
    "dravencms",
    "translation"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "kdyby/translation": "^v2.4.0"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "translation": "Kdyby\\Translation\\DI\\TranslationExtension"
        }
      }
    }
  }
}

